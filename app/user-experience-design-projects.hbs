    <meta property="og:url" content="https://rillusion.com/" />
    <meta property="og:url" content="https://www.rillusion.com/" />
    <meta property="og:site_name" content="Rillusion" />
    <meta property="og:type" content="website" />
    <meta property="og:image" content="./images/favicons/rillusion-1200.png" />
    <meta property="fb:app_id" content="546964375459684" />
    <meta property="fb:admins" content="100004519120778" />

    <!-- Schema.org markup for Google+ -->
    <meta itemprop="name" content="Rillusion - Contact us for UI UX Design & Product Design Projects">
    <meta itemprop="description" content="FinTech UX UI Design, Edu-Tech UX UI Design, E-Commerce UX UI Design and Development, Saas UX UI Design, Enterprise, Intranet Portal, CRM, ERP UX UI Design " />
    <meta itemprop="image" content="./images/favicons/rillusion-1200.png" />

    <!-- Twitter Card -->
    <meta name="twitter:card" content="summary" />
    <meta name="twitter:site" content="@weareRillusion" />
    <meta name="twitter:creator" content="@weareRillusion" />
    <meta name="twitter:title" content="Rillusion - Contact us for UI UX Design & Product Design Projects" />
